<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SearchActivity">

    <TextView
        android:id="@+id/textView44"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="24dp"
        android:fontFamily="@font/inter_semi_bold"
        android:gravity="center"
        android:text="@string/search_title"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextText7"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/custome_input"
        android:backgroundTint="@color/back_button"
        android:drawableStart="@drawable/baseline_search"
        android:drawablePadding="8dp"
        android:ems="10"
        android:hint="@string/input_plaesholder5"
        android:inputType="text"
        android:paddingStart="8dp"
        android:paddingEnd="5dp"
        android:state_enabled="false"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/frameLayout19"
        app:layout_constraintStart_toStartOf="@+id/textView44"
        app:layout_constraintTop_toBottomOf="@+id/textView44" />

    <FrameLayout
        android:id="@+id/frameLayout19"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:backgroundTint="@color/back_button"
        app:layout_constraintBottom_toBottomOf="@+id/editTextText7"
        app:layout_constraintEnd_toEndOf="@+id/textView44"
        app:layout_constraintTop_toTopOf="@+id/editTextText7">

        <ImageView
            android:id="@+id/imageView25"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:foregroundTint="@color/back_button"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/shape_all_view" />

        <ImageButton
            android:id="@+id/imageButton11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:backgroundTint="@color/back_button"
            app:srcCompat="@drawable/baseline_filter" />
    </FrameLayout>

    <com.google.android.material.chip.Chip
        android:id="@+id/chip_music_item"
        style="@style/Widget.Material3.Chip.Filter"
        android:layout_width="100dp"
        android:layout_height="55dp"
        android:layout_marginTop="8dp"
        android:backgroundTint="@color/back_button"
        android:checked="true"
        app:chipBackgroundColor="@color/back_button"
        app:chipStrokeColor="@color/back_button"
        app:layout_constraintStart_toStartOf="@+id/editTextText7"
        app:layout_constraintTop_toBottomOf="@+id/editTextText7" />

    <FrameLayout
        android:id="@+id/frameLayout20"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/chip_music_item"
        app:layout_constraintStart_toStartOf="@+id/chip_music_item"
        app:layout_constraintTop_toTopOf="@+id/chip_music_item">

        <ImageView
            android:id="@+id/imageView21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:tint="#FCF5FD"
            app:srcCompat="@drawable/shape_curicle" />

        <FrameLayout
            android:layout_width="25dp"
            android:layout_height="25dp">

            <ImageView
                android:id="@+id/imageView23"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                app:srcCompat="@drawable/music" />
        </FrameLayout>
    </FrameLayout>

    <TextView
        android:id="@+id/textView45"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/inter_semi_bold"
        android:text="@string/chipe_title1"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/chip_music_item"
        app:layout_constraintEnd_toEndOf="@+id/chip_music_item"
        app:layout_constraintStart_toEndOf="@+id/frameLayout20"
        app:layout_constraintTop_toTopOf="@+id/chip_music_item" />

    <com.google.android.material.chip.Chip
        android:id="@+id/chip"
        android:layout_width="100dp"
        android:layout_height="55dp"
        android:layout_marginStart="8dp"
        app:chipBackgroundColor="@color/back_button"
        app:chipStrokeColor="@color/back_button"
        app:layout_constraintBottom_toBottomOf="@+id/chip_music_item"
        app:layout_constraintStart_toEndOf="@+id/chip_music_item"
        app:layout_constraintTop_toTopOf="@+id/chip_music_item" />

    <FrameLayout
        android:id="@+id/frameLayout21"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/chip"
        app:layout_constraintStart_toStartOf="@+id/chip"
        app:layout_constraintTop_toTopOf="@+id/chip">

        <ImageView
            android:id="@+id/imageView24"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:tint="#FCF5FD"
            app:srcCompat="@drawable/shape_curicle" />

        <FrameLayout
            android:layout_width="25dp"
            android:layout_height="25dp">

            <ImageView
                android:id="@+id/imageView26"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                app:srcCompat="@drawable/painting" />
        </FrameLayout>
    </FrameLayout>

    <TextView
        android:id="@+id/textView46"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/inter_semi_bold"
        android:text="@string/chipe_title2"
        android:textSize="12dp"
        app:layout_constraintBottom_toBottomOf="@+id/frameLayout21"
        app:layout_constraintEnd_toEndOf="@+id/chip"
        app:layout_constraintStart_toEndOf="@+id/frameLayout21"
        app:layout_constraintTop_toTopOf="@+id/frameLayout21" />


</androidx.constraintlayout.widget.ConstraintLayout>